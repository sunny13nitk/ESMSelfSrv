{
  "welcomeFile": "index.html",
  "authenticationMethod": "route",
  "routes": [
    {
      "source": "^/esm/cases/:lob",
      "target": "/ess/",
      "type:":"proxy",
      "destination": "srv-api",
      "authenticationType": "xsuaa",
      "type":"proxy",
      "requestParameters": {
        "lobName": "$1"
      },
      "cacheControl": "no-cache, no-store, must-revalidate"
    },
    {
      "source": "^/post",
      "target": "/post",
      "destination": "srv-api",
      "authenticationType": "xsuaa",
      "cacheControl": "no-cache, no-store, must-revalidate",
      "csrfProtection": false
    },
    {
      "source": "^/logout",
      "target": "/logout",
      "destination": "srv-api",
      "authenticationType": "none"
    },
    {
      "source": "^/api",
      "target": "/api",
      "destination": "srv-api",
      "authenticationType": "xsuaa",
      "cacheControl": "no-cache, no-store, must-revalidate"
    },
    {
      "source": "^/login(.*)",
      "target": "/login$1",
      "destination": "srv-api",
      "authenticationType": "xsuaa",
      "cacheControl": "no-cache, no-store, must-revalidate"
    }
  ]
}